<div class="row">
  <div class="span12">
    <h1>All Users <small><a href="/users/create">create user</a></h1>
  </div>
</div>
<div class="row">
  <div class="span12">
    <ul>
      <% @users.each do |user| %>
        <li><%= user.name %> (<%= user.email %>) <a href="/users/<%= user.id %>/edit">edit</a></li>
      <% end %>
    </ul>
  </div>
</div>